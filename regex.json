  },
  {
    "secret": "Facebook_Access_Token",
    "pattern": "EAACEdEose0cBA[0-9A-Za-z]+"
  },
  {
    "secret": "Twitter_Secret_Key",
    "pattern": "([0-9a-zA-Z]{35,44})"
  },
  {
    "secret": "Stripe_API_Key",
    "pattern": "sk_live_[0-9a-zA-Z]{24}"
